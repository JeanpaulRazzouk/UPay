<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/prof"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/frag_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/imageButton"
            android:layout_width="106dp"
            android:layout_height="106dp"
            android:layout_marginStart="250dp"
            android:layout_marginTop="100dp"
            android:layout_marginEnd="0dp"
            android:background="@drawable/round_outline"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toTopOf="@+id/textView7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView7"
            app:layout_constraintVertical_bias="0.707" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="138dp"
            android:layout_height="29dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="135dp"
            android:layout_marginBottom="512dp"
            android:fontFamily="@font/sf_ui_display_black"
            android:text="TextView"
            android:textSize="18dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="235dp"
            android:layout_height="62dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="150dp"
            android:fontFamily="@font/sf_ui_display_black"
            android:text="TextView"
            android:textColor="#225E8C"
            android:textSize="36sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView" />


        <TextView
            android:id="@+id/textView3"
            android:layout_width="222dp"
            android:layout_height="49dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="47dp"
            android:fontFamily="@font/sf_ui_display_black"
            android:text="@string/profile"
            android:textColor="#CB090808"
            android:textSize="36sp"
            app:layout_constraintBottom_toTopOf="@+id/textView"
            app:layout_constraintEnd_toStartOf="@+id/imageButton"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ScrollView
            android:id="@+id/scroll"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="275dp"
            android:background="@null"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="?android:windowBackground"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="115dp"
                    android:layout_marginStart="25dp"
                    android:layout_marginEnd="25dp"
                    app:cardBackgroundColor="#E8EBEC"
                    app:cardCornerRadius="16dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.432">

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/sf_ui_display_black"
                        android:gravity="center"
                        android:text="Settings"
                        android:textSize="18sp" />

                    <ImageButton
                        android:id="@+id/pers_info_btn"
                        android:layout_width="match_parent"
                        android:layout_height="115dp"
                        android:layout_gravity="bottom"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="50dp"
                        android:layout_marginEnd="0dp"
                        android:layout_marginBottom="0dp"
                        android:background="@null"
                        android:onClick="Pers_info" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="115dp"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="25dp"
                    app:cardBackgroundColor="#E8EBEC"
                    app:cardCornerRadius="16dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.432">

                    <TextView
                        android:id="@+id/textView101"
                        android:layout_width="341dp"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/sf_ui_display_black"
                        android:gravity="center"
                        android:text="Feedback"
                        android:textSize="18sp" />

                    <ImageButton
                        android:id="@+id/imageButton13"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:onClick="feedback"/>

                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="115dp"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginEnd="25dp"
                    app:cardBackgroundColor="#E8EBEC"
                    app:cardCornerRadius="16dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.432">

                    <TextView
                        android:id="@+id/textView104"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/sf_ui_display_black"
                        android:gravity="center"
                        android:text="UPay"
                        android:textSize="18sp" />

                    <ImageButton
                        android:id="@+id/terms_of_service_btn"
                        android:layout_width="match_parent"
                        android:layout_height="115dp"
                        android:layout_gravity="bottom"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="50dp"
                        android:layout_marginEnd="0dp"
                        android:layout_marginBottom="0dp"
                        android:background="@null"
                        android:onClick="terms_service" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/log_out"
                    android:layout_width="99dp"
                    android:layout_height="52dp"
                    android:layout_marginStart="150dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="102dp"
                    android:layout_marginBottom="10dp"
                    app:cardBackgroundColor="#DC3333"
                    app:cardCornerRadius="16dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.432">

                    <ImageButton
                        android:id="@+id/log_out_btn"
                        android:layout_width="match_parent"
                        android:layout_height="52dp"
                        android:layout_gravity="bottom"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="50dp"
                        android:layout_marginEnd="0dp"
                        android:layout_marginBottom="0dp"
                        android:background="@null"
                        android:onClick="logout" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="101dp"
                        android:layout_height="51dp"
                        android:fontFamily="@font/sf_ui_display_black"
                        android:gravity="center"
                        android:text="Logout"
                        android:textColor="#FAF1F1"
                        android:textSize="18sp" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

            </LinearLayout>
        </ScrollView>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
