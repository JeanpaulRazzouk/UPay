<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?android:windowBackground"
    tools:context="com.example.Profile.PersonalInfoFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".HomePage">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recycle2"
            android:layout_width="399dp"
            android:layout_height="466dp"
            android:layout_marginTop="141dp"
            android:layout_marginBottom="37dp"
            app:layout_constraintBottom_toTopOf="@+id/log_out"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.cardview.widget.CardView
            android:id="@+id/log_out"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginBottom="162dp"
            app:cardBackgroundColor="#DC3333"
            app:cardCornerRadius="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/recycle2">

            <ImageButton
                android:id="@+id/dlt_btn"
                android:layout_width="match_parent"
                android:layout_height="52dp"
                android:layout_gravity="bottom"
                android:background="@null"
                tools:layout_editor_absoluteX="152dp"
                tools:layout_editor_absoluteY="571dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:fontFamily="@font/sf_ui_display_black"
                android:gravity="center"
                android:text="Delete account"
                android:textColor="#FAF1F1"
                android:textSize="15sp"
                tools:layout_editor_absoluteX="144dp"
                tools:layout_editor_absoluteY="569dp" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="224dp"
            android:fontFamily="@font/sf_ui_display_black"
            android:text="Settings"
            android:textColor="@color/defaultContentColor"
            android:textSize="36dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>